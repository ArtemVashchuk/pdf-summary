{
  "name": "pdf-summary-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "PDF Summmary Backend Service",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --format=esm --outdir=dist --external:better-sqlite3 --external:pg-native --external:canvas --external:sharp --external:@google/generative-ai --external:jsqr --banner:js=\"import { createRequire } from 'module'; const require = createRequire(import.meta.url);\"",
    "start": "npm run db:push && node dist/index.js",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "@google/generative-ai": "latest",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "memorystore": "^1.6.7",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "sharp": "^0.33.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.0",
    "drizzle-kit": "^0.31.4",
    "tsx": "^4.20.5",
    "typescript": "5.6.3",
    "esbuild": "^0.20.0"
  }
}